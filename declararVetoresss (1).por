programa {
  funcao inicio() {
    inteiro notas[10],i//vetor com 10 posições
    para(i=0; i<10; i++){//um loop controlado que define o valor de todos os slots do vetor notas
      escreva("nota ",i,": ",notas[i]"\n")
      leia(notas[i])
    }
    escreva(notas[0]," ",notas[1]," ",notas[2]," ",notas[3]," ",notas[4]," ",notas[5]," ",notas[6]," ",notas[7]," ",notas[8]," ",notas[9]," ",)
  }
}
